<% include common/header %>

  <!-- write your content inside this div -->

  <div class="content-container">
    <div class="content">
      <div class="row">
        <div class="col-md-8 col-md-offset-2 menu-container">
          <% include common/account-menu %>
        </div>
      </div>
      <div class="row">
        <div class="col-md-8 col-md-offset-2">
        
            <h2>Here are voice submissions of <%= form.formMeta.title%></h2> 
            
            <table class="submission">
            	<thead>
            		<tr>
            			<th>#</th><th>Question</th><th>Answer(Text)</th><th>Answer(Voice)</th>
            		</tr>
            	</thead>
            	<tbody>
		            <% for (el in subs) { var sub = subs[el];%>
		            	<% if(el !== "callDetails" && el !== "number"){%>
		            		<tr>
		            			<td><%= el%></td><td><%= form.qs[el].text%></td><td><%=sub.text %></td><td><a href="#" onclick="$('#audio-<%=el%>').toggle();return false;">listen</a> 
                                                                                                                          <div id="audio-<%=el%>" class="dn">
                                                                                                                                <audio src="<%=sub.audio %>.mp3" preload="auto" />
                                                                                                                          </div></td>
		            		</tr>
						<%}//endif%>
					<% }//endfor%>
            	</tbody>
        	</table>
            <script type="text/javascript">


            </script>
              
        </div>
      </div>
    </div>
  </div>

<% include common/footer %>